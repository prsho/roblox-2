			<Item class="Script" referent="RBXCC1E6FF2958640CEB5FCCF0F48C183A8">
				<Properties>
					<bool name="Disabled">false</bool>
					<Content name="LinkedSource"><null></null></Content>
					<string name="Name">LERP</string>
					<ProtectedString name="Source"><![CDATA[--//Prsho
script.Parent.Crackle:play()
function lerp(a,b,t) --Linear Interpolation (LERP)
	local r = a.r+t*(b.r-a.r)
	local g = a.g+t*(b.g-a.g)
	local b = a.b+t*(b.b-a.b)
	return Color3.new(r/255,g/255,b/255)
end
math.randomseed(tick()+math.random(999999)+math.random(435646)+math.random(7855467))
time_ = 5+math.random(5)
bin = script.Parent
light = bin.PointLight
fire = bin.Fire
bloom = bin.Bloom.ImageLabel
coroutine.resume(coroutine.create(function()
	while true do
		wait(0.1)
		local v = math.random(50,75)/100
		light.Brightness = v
		bloom.Parent.Size = UDim2.new(v*14,0,v*14,0)
	end
end))
while true do
	script.Parent.Crackle.Volume = bin.Size.magnitude/10
	for f=1,time_*10 do
		local f = f/(time_*10)
		light.Color = lerp(Color3.new(212, 110, 28),Color3.new(255, 179, 0),f)
		light.Range = (bin.Size.magnitude*30)+(f*8)
		bloom.ImageColor3 = light.Color
		fire.Heat = 4*(bin.Size.magnitude+f)
		script.Parent.Crackle.Pitch = math.random(100,140)/100
		fire.Color = Color3.new(light.Color.r,0.5*(light.Color.g+0.5),0.5*(light.Color.b))
		fire.SecondaryColor = Color3.new(light.Color.r,0.5*(light.Color.g),0.5*(light.Color.b))
		wait(0.1)
	end
	for f=1,time_*10 do
		local f = f/(time_*10)
		light.Color = lerp(Color3.new(255, 179, 0),Color3.new(212, 110, 28),f)
		light.Range = (8+(bin.Size.magnitude*30))-(f*8)
		bloom.ImageColor3 = light.Color
		fire.Heat = (4+(bin.Size.magnitude*4))-(f*4)
		script.Parent.Crackle.Pitch = math.random(100,140)/100
		fire.Color = Color3.new(light.Color.r,0.5*(light.Color.g+0.5),0.5*(light.Color.b))
		fire.SecondaryColor = Color3.new(light.Color.r,0.5*(light.Color.g),0.5*(light.Color.b))
		wait(0.1)
	end
end]]></ProtectedString>
